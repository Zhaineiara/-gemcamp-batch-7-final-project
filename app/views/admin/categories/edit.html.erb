<%= form_with(model: @category, url: admin_category_path, method: :put, local: true) do |form| %>
  <div class="form-group">
    <%= form.label :name, "Category Name" %>
    <%= form.text_field :name %>
  </div>

  <div class="form-group">
    <%= form.submit "Create Category", class: "btn btn-primary" %>
  </div>
<% end %>
