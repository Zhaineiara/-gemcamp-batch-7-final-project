<h1>Admin Dashboard</h1>
<p>Find me in app/views/admin/home/index.html.erb</p>

<% if flash[:notice] %>
  <p class="flash notice"><%= flash[:notice] %></p>
<% end %>

<% if flash[:alert] %>
  <p class="flash alert"><%= flash[:alert] %></p>
<% end %>

<% if admin_user_signed_in? %>
  <%= link_to 'Sign out', destroy_admin_user_session_path, data: { 'turbo-method': :delete } %>
<% else %>
  <%= link_to 'LOG IN ADMIN', new_admin_user_session_path %>
<% end %>